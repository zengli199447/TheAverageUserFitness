<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.journeyapps.barcodescanner.BarcodeView
        android:id="@+id/zxing_barcode_surface"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:zxing_framing_rect_height="@dimen/dp200"
        app:zxing_framing_rect_width="@dimen/dp200" />

    <com.example.administrator.sportsFitness.ui.view.ZxingViewFinderView
        android:id="@+id/zxing_viewfinder_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:scannerBoundColor="@color/gray_"
        app:scannerBoundCornerColor="@color/blue_bar"
        app:scannerBoundCornerHeight="24dp"
        app:scannerBoundCornerWith="1.5dp"
        app:scannerBoundWidth="0.5dp"
        app:scannerLaserResId="@drawable/laser_icon"
        app:scannerTipText="@string/qr_prompt"
        app:scannerTipTextColor="@color/white"
        app:scannerTipTextMargin="@dimen/dp30"
        app:scannerTipTextSize="@dimen/dp13"
        app:zxing_possible_result_points="@color/blue_bar"
        app:zxing_result_view="@color/zxing_custom_result_view"
        app:zxing_viewfinder_laser="@color/blue_bar"
        app:zxing_viewfinder_mask="@color/translucent" />

</merge>


